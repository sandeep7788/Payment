                            app:cardCornerRadius="4dp"
                            android:gravity="center"
                            android:shadowColor="@color/gray_btn_bg_color"
                            android:orientation="vertical"
                            android:layout_marginStart="12dp"
                            android:layout_marginEnd="12dp"
                            android:layout_marginBottom="12dp">

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:layout_width="match_parent"
                                android:background="@drawable/border"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:id="@+id/txtMsg1"
                                    android:layout_width="match_parent"
                                    android:layout_marginTop="1dp"
                                    android:layout_marginStart="16dp"
                                    android:layout_height="wrap_content"
                                    android:textColorHint="@color/gray_btn_bg_color"
                                    android:backgroundTint="@color/white"
                                    android:hint="Please select your Service Operator."
                                    android:padding="4dp"
                                    android:textColor="@color/text_color"
                                    android:textSize="@dimen/text"
                                    android:layout_marginEnd="49dp"
                                    android:gravity="center|start"
                                    android:layout_gravity="center"
                                    android:fontFamily="@font/intel"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.0"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <ImageView
                                    app:tint="@color/gray_color"
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:layout_marginEnd="8dp"
                                    android:foregroundGravity="center"
                                    android:gravity="center"
                                    android:padding="8dp"
                                    android:src="@drawable/baseline_info_24"
                                    android:textSize="16dp"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="1.0"
                                    app:layout_constraintTop_toTopOf="parent" />

                            </androidx.constraintlayout.widget.ConstraintLayout>



                        </androidx.cardview.widget.CardView>


                        <androidx.cardview.widget.CardView
                            android:id="@+id/card_knumber"
                            android:layout_width="match_parent"
                            android:elevation="8dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            app:cardCornerRadius="4dp"
                            android:gravity="center"
                            android:shadowColor="@color/gray_btn_bg_color"
                            android:orientation="vertical"
                            android:layout_marginStart="12dp"
                            android:layout_marginEnd="12dp"
                            >

                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/l2"
                                android:layout_width="match_parent"
                                android:background="@drawable/border"
                                android:layout_height="55dp">

                                <EditText
                                    android:id="@+id/edtKNumber"
                                    android:layout_width="match_parent"
                                    android:layout_marginTop="4dp"
                                    android:layout_marginStart="16dp"
                                    android:textColor="@color/text_color"
                                    android:paddingStart="8dp"
                                    android:layout_height="45dp"
                                    android:textColorHint="@color/gray_btn_bg_color"
                                    android:backgroundTint="@color/white"
                                    android:hint="Enter K Number"
                                    android:textSize="@dimen/text"
                                    android:layout_gravity="center"
                                    android:fontFamily="@font/intel"
                                    android:inputType="text"
                                    android:lines="1"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="0.0"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <ImageView
                                    android:id="@+id/image10"
                                    android:layout_width="45dp"
                                    android:layout_height="45dp"
                                    android:layout_marginEnd="8dp"
                                    android:foregroundGravity="center"
                                    android:gravity="center"
                                    android:padding="8dp"
                                    android:src="@drawable/ic_elec"
                                    android:textSize="16dp"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintHorizontal_bias="1.0"
                                    app:layout_constraintStart_toStartOf="@+id/edtKNumber"
                                    app:layout_constraintTop_toTopOf="parent"
                                    />

                            </androidx.constraintlayout.widget.ConstraintLayout>



                        </androidx.cardview.widget.CardView>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="right"
    android:gravity="right"
    android:layout_marginBottom="4dp"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_fetch"
        android:layout_width="120dp"
        android:elevation="4dp"
        android:layout_height="30dp"
        android:layout_gravity="center"

        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="4dp"
        android:background="@drawable/blue_button_background"
        android:fontFamily="@font/baloo"
        android:foregroundGravity="right"
        android:gravity="center"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:text="Fetch"
        android:textColor="#ffffff"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



</androidx.constraintlayout.widget.ConstraintLayout>


